{
  "name": "techstack",
  "version": "1.0.0",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress && yarn autoprefixer",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "lint": "eslint src webpack.config.babel.js --fix --ext .js,.jsx",
    "test": "yarn lint && jest --coverage && flow",
    "prepush": "yarn test && yarn prod:build",
    "autoprefixer": "postcss --use autoprefixer --autoprefixer.browsers '&gt; 1%, ie 9' -o dist/css/styles.css dist/css/styles.css"
  },
  "dependencies": {
    "autoprefixer": "^8.6.0",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "immutable": "4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "postcss-cli": "^5.0.0",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-actions": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-css-modules-transform": "^1.6.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-compat": "^2.3.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.73.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "pm2": "^2.10.4",
    "prettier-eslint": "^8.8.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
